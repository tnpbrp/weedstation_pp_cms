{
    "version": 2,
    "public": true,
    "build": {
        "functions": {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        "config": {
            "src": "config/**/*.js",
            "use": "@vercel/node"
        },
        "app": {
            "src": "app.js",
            "use": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}
